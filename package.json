{
  "name": "stickman-battle",
  "version": "1.0.0",
  "description": "Stickman fighting game with campaign mode",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["game", "stickman", "fighting", "canvas"],
  "author": "Truonghuu478",
  "license": "MIT",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": ["./tests/setup.js"],
    "collectCoverageFrom": [
      "js/**/*.js",
      "!js/main.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
